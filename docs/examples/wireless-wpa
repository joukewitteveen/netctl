Description='A simple WPA encrypted wireless connection'
Interface=wlan0
Connection=wireless
Security=wpa
IP=dhcp
ESSID='MyNetwork'

# ASCII Keys use quoted method.
# 256-bit PSK use non-quoted method.
# Keys using special characters use quoted string method.
#Key='ASCIIWirelessKey'
#Key='/EncryptedPSK
#Key='""WirelessKey"'

